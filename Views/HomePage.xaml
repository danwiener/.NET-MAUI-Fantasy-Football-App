<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FantasyFootballMAUI.HomePage"
             BackgroundColor="#e6e6e6"
             Title=""
             xmlns:Views="clr-namespace:FantasyFootballMAUI"
             x:Name="homePage">
    <ContentPage.Resources>
        <Style TargetType="Grid">
            <Setter Property="VisualStateManager.VisualStateGroups">
                <VisualStateGroupList>
                    <VisualStateGroup x:Name="CommonStates">
                        <VisualState x:Name="Normal">
                        </VisualState>
                        <VisualState x:Name="Selected">
                            <VisualState.Setters>
                                <Setter Property="BackgroundColor"
                                        Value="#6588a6"/>
                            </VisualState.Setters>
                        </VisualState>
                    </VisualStateGroup>
                </VisualStateGroupList>
            </Setter>
        </Style>
    </ContentPage.Resources>

    <Grid x:Name="HomeGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height=".25*"/>
            <RowDefinition Height="44"/>
            <RowDefinition Height=".5*"/>
            <RowDefinition Height="44"/>
            <RowDefinition Height=".25*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width=".05*"/>
            <ColumnDefinition Width=".9*"/>
            <ColumnDefinition Width=".05*"/>
        </Grid.ColumnDefinitions>


        <!--<Grid Grid.RowSpan="5" Grid.ColumnSpan="3" RowDefinitions=".5*,.5*" ZIndex="0">
            <BoxView Color="Black" Grid.Row="1"/>
        </Grid>-->

        <Label x:Name="UsernameLabel" Grid.Row="0" FontAttributes="Bold" FontFamily="PT" Grid.Column="0" Grid.ColumnSpan="3" HorizontalOptions="Center" VerticalOptions="Center" TextColor="#0d0000" CharacterSpacing="2" />



        <Grid Grid.Row="1" Grid.RowSpan="1" Grid.Column="1" ColumnDefinitions=".5*,.5*" VerticalOptions="End" Margin="0,0,0,-5">
            <Label x:Name="TitleLabel1" Grid.Column="0" HorizontalTextAlignment="Center" VerticalTextAlignment="Center" Text="LEAGUES BELONGED TO" FontSize="22" FontAttributes="Bold" FontFamily="PT" FontAutoScalingEnabled="True" TextColor="Black" HorizontalOptions="Center" VerticalOptions="Center" Scale="3">
                <Label.Shadow>
                    <Shadow Brush="#ffffff" Offset="0,20" Radius="20" Opacity="1.2"/>
                </Label.Shadow>
            </Label>

            <Label x:Name="TitleLabel2" Grid.Column="1" HorizontalTextAlignment="Center" VerticalTextAlignment="Center" Text="MY TEAMS" FontSize="22" FontAttributes="Bold" FontFamily="PT" FontAutoScalingEnabled="True" TextColor="Black" HorizontalOptions="Center" VerticalOptions="Center" Scale="3">
                <Label.Shadow>
                    <Shadow Brush="#ffffff" Offset="0,20" Radius="20" Opacity="1.2"/>
                </Label.Shadow>
            </Label>
        </Grid>


        <Grid Grid.Column="1" Grid.Row="3" ColumnDefinitions=".5*,.5*" Margin="0,5,0,0" HorizontalOptions="Start">
            <HorizontalStackLayout Grid.Column="0" VerticalOptions="Center" Grid.ColumnSpan="1" HorizontalOptions="Start" Margin="7.5,0,0,0">
                <Button Text="VIEW LEAGUE" CornerRadius="0" x:Name="ViewLeagueBtn" Clicked="OnViewLeagueClicked" HeightRequest="44" WidthRequest="90" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6" BorderColor="#ffffff" BorderWidth="1" TextColor="Black" Margin="0, 0, 0, 0"/>
                <Button Text="GO BACK" CornerRadius="0" x:Name="GoBackBtn" IsVisible="false" Clicked="OnGoBackClicked" HeightRequest="44" WidthRequest="90" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6" BorderColor="#ffffff" BorderWidth="1" TextColor="Black"  Margin="0, 0, 0, 0"/>
                <Button Text="VIEW FREE AGENTS" CornerRadius="0" IsVisible="false" x:Name="ViewFreeAgentsBtn" Clicked="OnViewFreeAgentsClicked" HeightRequest="44" WidthRequest="102.5" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BorderColor="#ffffff" BorderWidth="1" BackgroundColor="#e6e6e6" TextColor="Black" Margin="0, 0, 0, 0"/>
                <Button Text="GO BACK" x:Name="GoBackFABtn" CornerRadius="0" IsVisible="false" Clicked="OnGoBackFAClicked" HeightRequest="44" WidthRequest="102.5" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BorderColor="#ffffff" BorderWidth="1" BackgroundColor="#e6e6e6" TextColor="Black"  Margin="0, 0, 0, 0"/>


                <Button Text="DELETE LEAGUE" CornerRadius="0" x:Name="DeleteBtn" Padding="-5" Clicked="OnDeleteLeagueClicked" HeightRequest="44" WidthRequest="90" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6"  BorderColor="#ffffff" BorderWidth="1" TextColor="Black" Margin="2.5, 0, 0, 0"/>
                <Button Text="EDIT" CornerRadius="0" IsVisible="false" x:Name="EditBtn" Padding="-5" Clicked="OnEditBtnClicked" HeightRequest="44" WidthRequest="90" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6"  BorderColor="#ffffff" BorderWidth="1" TextColor="Black" Margin="2.5, 0, 0, 0"/>
                <Button Text="ENTER" CornerRadius="0" IsVisible="false" x:Name="EnterRulesBtn" Padding="-5" Clicked="OnEnterBtnClicked" HeightRequest="44" WidthRequest="90" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6"  BorderColor="#ffffff" BorderWidth="1" TextColor="Black" Margin="2.5, 0, 0, 0"/>
                <Button Text="ENTER" CornerRadius="0" IsVisible="false" x:Name="EnterLeagueBtn" Padding="-5" Clicked="OnEnterLeagueBtnClicked" HeightRequest="44" WidthRequest="90" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6"  BorderColor="#ffffff" BorderWidth="1" TextColor="Black" Margin="2.5, 0, 0, 0"/>
                <Button Text="DROP PLAYER" CornerRadius="0" IsVisible="false" x:Name="DropBtn" Padding="-5" Clicked="OnDropPlayerClicked" HeightRequest="44" WidthRequest="90" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6"  BorderColor="#ffffff" BorderWidth="1" TextColor="Black" Margin="2.5, 0, 0, 0"/>
                <Button Text="ADD PLAYER" CornerRadius="0" IsVisible="false" x:Name="AddBtn" Padding="-5" Clicked="OnAddPlayerClicked" HeightRequest="44" WidthRequest="90" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6"  BorderColor="#ffffff" BorderWidth="1" TextColor="Black" Margin="2.5, 0, 0, 0"/>


                <Button BorderColor="#ffffff" CornerRadius="0" BorderWidth="1" Text="JOIN LEAGUE" x:Name="JoinLeagueBtn" IsVisible="false" Clicked="OnJoinLeagueClicked" HeightRequest="44" WidthRequest="90" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6" TextColor="Black"  Margin="2.5, 0, 0, 0"/>
                <Button BorderColor="#ffffff" CornerRadius="0" BorderWidth="1" Text="VIEW/EDIT RULES" x:Name="RulesBtn" IsVisible="false" Padding="-5" Clicked="OnViewEditRulesClicked" HeightRequest="44" WidthRequest="87.5" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6" TextColor="Black" Margin="2.5, 0, 0, 0"/>

                <Button BorderColor="#ffffff" CornerRadius="0" BorderWidth="1" x:Name="CreateLeagueBtn" IsVisible="False" Text="CREATE LEAGUE" Clicked="OnCreateLeagueClicked" HeightRequest="44" WidthRequest="90" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6" TextColor="Black" Margin="2.5, 0, 0, 0"/>
                <Button BorderColor="#ffffff" CornerRadius="0" BorderWidth="1" Text="GO BACK"  x:Name="GoBackBtn2"  IsVisible="false" Clicked="OnGoBack2Clicked" HeightRequest="44" WidthRequest="87.5" VerticalOptions="Center" HorizontalOptions="End" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6" TextColor="Black"  Margin="2.5, 0, 2.5, 0"/>

                <Label x:Name="OrLabel" FontSize="20" Text="OR"  TextColor="#6588a6" Margin="6.25, 0, 3.75, 0" WidthRequest="30" VerticalOptions="Center"/>
                <Button BorderColor="#ffffff" CornerRadius="0" BorderWidth="1" x:Name="JoinCreateBtn" Text="JOIN/CREATE OTHER LEAGUE" Clicked="OnJoinOtherClicked"  HeightRequest="44" WidthRequest="140" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6" TextColor="Black" Margin="2.5, 0, 0, 0"/>
            </HorizontalStackLayout>
        </Grid>







        <Grid Grid.Column="1" Grid.Row="2" Grid.RowSpan="1" ZIndex="1" x:Name="GlobalLeaguesGrid" IsVisible="False">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width=".5*"/>
                <ColumnDefinition Width=".5*"/>
            </Grid.ColumnDefinitions>

            <CollectionView x:Name="GlobalLeaguesCollectionView" IsEnabled="False" Grid.Column="0" Grid.Row="0" WidthRequest="380" HorizontalOptions="StartAndExpand" SelectionMode="Single" SelectionChanged="GlobalLeaguesCollectionView_SelectionChanged" ZIndex="5" VerticalOptions="Center" EmptyView="No leagues available. Be the first, create one!">
                <CollectionView.ItemsLayout>
                    <GridItemsLayout Orientation="Vertical" Span="1"/>
                </CollectionView.ItemsLayout>
                <CollectionView.ItemTemplate>
                    <DataTemplate>
                        <Grid Padding="10" VerticalOptions="Center">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="44"/>
                                <RowDefinition Height="44"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width=".1*"/>
                                <ColumnDefinition Width=".8*"/>
                                <ColumnDefinition Width=".1*"/>
                            </Grid.ColumnDefinitions>


                            <Grid ColumnDefinitions="35,*" RowDefinitions="*" Grid.Row="0" Grid.Column="1">
                                <Image Source="league.png" HeightRequest="25" WidthRequest="25" Grid.Column="0" ZIndex="5" Margin="2.5,0,-5,0"/>
                                <Entry HeightRequest="38" Grid.Row="0" IsEnabled="false" Grid.Column="0" Grid.ColumnSpan="1" VerticalOptions="Center" HorizontalOptions="FillAndExpand" BackgroundColor="#f2f2f2" TextColor="Black" ZIndex="3" PlaceholderColor="Black" VerticalTextAlignment="Center"/>
                                <Entry HeightRequest="38" Grid.Row="0" IsEnabled="false" Grid.Column="1" VerticalOptions="Center" HorizontalTextAlignment="Start" HorizontalOptions="FillAndExpand" BackgroundColor="#f2f2f2" TextColor="Black" ZIndex="5" Placeholder="{Binding LeagueNameStr}" PlaceholderColor="Black" VerticalTextAlignment="Center"/>
                            </Grid>



                            <Grid ColumnDefinitions="*,35" RowDefinitions="*" Grid.Row="1" Grid.Column="1">
                                <Entry HeightRequest="38" IsEnabled="False" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" VerticalOptions="Center" HorizontalOptions="FillAndExpand" BackgroundColor="#f2f2f2" TextColor="Black" ZIndex="5" Placeholder="I Am Commissioner" PlaceholderColor="Black" VerticalTextAlignment="Center"/>
                                <CheckBox BackgroundColor="#6588a6" Color="#6588a6" IsEnabled="False" IsChecked="{Binding CreatedByCurrentUser}" VerticalOptions="Center" ZIndex="9" Grid.Column="1" HorizontalOptions="Start" WidthRequest="32" HeightRequest="32"/>
                            </Grid>

 

                        </Grid>

                    </DataTemplate>
                </CollectionView.ItemTemplate>
            </CollectionView>
        </Grid>



        <Grid Grid.Column="1" Grid.Row="2" Grid.RowSpan="1" ZIndex="1" x:Name="LeaguesBelongedToGrid">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width=".5*"/>
                <ColumnDefinition Width=".5*"/>
            </Grid.ColumnDefinitions>

            <CollectionView x:Name="LeaguesBelongedToCollectionView" Grid.Column="0" Grid.Row="0" WidthRequest="380" HorizontalOptions="StartAndExpand" SelectionMode="Single" SelectionChanged="LeaguesBelongedToCollectionView_SelectionChanged" ZIndex="5" VerticalOptions="Center" EmptyView="No leagues belonged to. Join or create now!">
                <CollectionView.ItemsLayout>
                    <GridItemsLayout Orientation="Vertical" Span="1"/>
                </CollectionView.ItemsLayout>
                <CollectionView.ItemTemplate>
                    <DataTemplate>
                        <Grid Padding="10" VerticalOptions="Center">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="44"/>
                                <RowDefinition Height="44"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width=".1*"/>
                                <ColumnDefinition Width=".8*"/>
                                <ColumnDefinition Width=".1*"/>
                            </Grid.ColumnDefinitions>


                            <!--<BoxView Color="#6588a6" Grid.Row="0" Grid.RowSpan="1" Grid.Column="0" Grid.ColumnSpan="3">
                                <BoxView.Shadow>
                                    <Shadow Brush="black" Offset="0,10" Radius="20" Opacity="0.8"/>
                                </BoxView.Shadow>
                            </BoxView>

                            <BoxView Color="#6588a6" Grid.Row="3" Grid.RowSpan="1" Grid.Column="0" Grid.ColumnSpan="3">
                                <BoxView.Shadow>
                                    <Shadow Brush="black" Offset="0,10" Radius="20" Opacity="0.8"/>
                                </BoxView.Shadow>
                            </BoxView>-->


                            <Grid ColumnDefinitions="35,*" RowDefinitions="*" Grid.Row="0" Grid.Column="1">
                                <Image Source="league.png" HeightRequest="25" WidthRequest="25" Grid.Column="0" ZIndex="5" Margin="2.5,0,-5,0"/>
                                <Entry HeightRequest="38" Grid.Row="0" IsEnabled="false" Grid.Column="0" Grid.ColumnSpan="1" VerticalOptions="Center" HorizontalOptions="FillAndExpand" BackgroundColor="#f2f2f2" TextColor="Black" ZIndex="3" PlaceholderColor="Black" VerticalTextAlignment="Center"/>
                                <Entry HeightRequest="38" Grid.Row="0" IsEnabled="false" Grid.Column="1" VerticalOptions="Center" HorizontalTextAlignment="Start" HorizontalOptions="FillAndExpand" BackgroundColor="#f2f2f2" TextColor="Black" ZIndex="5" Placeholder="{Binding LeagueNameStr}" PlaceholderColor="Black" VerticalTextAlignment="Center"/>
                            </Grid>



                            <Grid ColumnDefinitions="*,35" RowDefinitions="*" Grid.Row="1" Grid.Column="1">
                                <Entry HeightRequest="38" IsEnabled="False" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" VerticalOptions="Center" HorizontalOptions="FillAndExpand" BackgroundColor="#f2f2f2" TextColor="Black" ZIndex="5" Placeholder="I Am Commissioner" PlaceholderColor="Black" VerticalTextAlignment="Center"/>
                                <CheckBox BackgroundColor="#6588a6" Color="#6588a6" IsEnabled="False" IsChecked="{Binding CreatedByCurrentUser}" VerticalOptions="Center" ZIndex="9" Grid.Column="1" HorizontalOptions="Start" WidthRequest="32" HeightRequest="32"/>
                            </Grid>


                        </Grid>

                    </DataTemplate>
                </CollectionView.ItemTemplate>
            </CollectionView>


            <!--FOOTBALL-->
            <!--
                                <Grid Grid.Column="2" Grid.ColumnSpan="2" Grid.Row="1" RowDefinitions=".2*,.2*,.2*,.2*,.2*"  HorizontalOptions="Center" ZIndex="8" Rotation="90">
                                    <BoxView Color="#D9d5d2" Grid.Column="0" Grid.Row="0" WidthRequest="19" HeightRequest="76" Rotation="95" Grid.ColumnSpan="1" CornerRadius="10,10,10,10"/>
                                    <BoxView Color="#D9d5d2" Grid.Column="0" Grid.Row="1" WidthRequest="19" HeightRequest="76" Rotation="95" Grid.ColumnSpan="1" CornerRadius="10,10,10,10" />
                                    <BoxView Color="#D9d5d2" Grid.Column="0" Grid.Row="2" WidthRequest="19" HeightRequest="76" Rotation="95" Grid.ColumnSpan="1" CornerRadius="10,10,10,10"/>
                                    <BoxView Color="#D9d5d2" Grid.Column="0" Grid.Row="3" WidthRequest="19" HeightRequest="76" Rotation="95" Grid.ColumnSpan="1" CornerRadius="10,10,10,10" />
                                    <BoxView Color="#D9d5d2" Grid.Column="0" Grid.Row="4" WidthRequest="19" HeightRequest="76" Rotation="95" Grid.ColumnSpan="1" CornerRadius="10,10,10,10"/>
                                </Grid>

                                <Grid Grid.Row="1" Grid.RowSpan="1" Grid.Column="1" Grid.ColumnSpan="4" ColumnDefinitions=".10*,.8*,.10*"  RowDefinitions=".5*,.5*" ZIndex="8">
                                    <BoxView Color="#D9d5d2" Grid.Row="0" Grid.RowSpan="1" Grid.Column="1" HeightRequest="11.4"  Margin="0,1,0,1" ZIndex="7" CornerRadius="5,5,5,5" VerticalOptions="EndAndExpand"  />
                                    <BoxView Color="#D9d5d2" Grid.Row="1" Grid.RowSpan="1" Grid.Column="1" HeightRequest="11.4"  Margin="0,1,0,1" ZIndex="7" CornerRadius="5,5,5,5" VerticalOptions="StartAndExpand"/>

                                </Grid>
                                <Ellipse Fill="#825736" Stroke="#825736" StrokeThickness="4" HeightRequest="204.16" WidthRequest="350" Grid.Column="1" Grid.ColumnSpan="4" HorizontalOptions="Center" Grid.Row="1" Grid.RowSpan="1" ZIndex="6" Opacity=".5"/>-->

        </Grid>


        <Grid Grid.Column="1" Grid.Row="2" Grid.RowSpan="1" ZIndex="1" x:Name="TeamsBelongedToGrid">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width=".5*"/>
                <ColumnDefinition Width=".5*"/>
            </Grid.ColumnDefinitions>

            <CollectionView x:Name="TeamsBelongedToCollectionView" Grid.Column="1" Grid.Row="0" WidthRequest="380" HorizontalOptions="StartAndExpand" SelectionMode="Single" SelectionChanged="TeamsBelongedToCollectionView_SelectionChanged" ZIndex="5" VerticalOptions="Center" EmptyView="No teams belonged to. Please create one at this time.">
                <CollectionView.ItemsLayout>
                    <GridItemsLayout Orientation="Vertical" Span="1"/>
                </CollectionView.ItemsLayout>
                <CollectionView.ItemTemplate>
                    <DataTemplate>
                        <Grid Padding="10" VerticalOptions="Center">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="44"/>
                                <RowDefinition Height="44"/>
                                <RowDefinition Height="44"/>
                                <RowDefinition Height="44"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width=".1*"/>
                                <ColumnDefinition Width=".8*"/>
                                <ColumnDefinition Width=".1*"/>
                            </Grid.ColumnDefinitions>


                                <Entry HeightRequest="38" Grid.Row="0" Grid.Column="1" IsEnabled="False" VerticalOptions="Center" HorizontalOptions="FillAndExpand" BackgroundColor="#f2f2f2" TextColor="Black" ZIndex="5" Placeholder="{Binding TeamNameStr}" PlaceholderColor="Black" VerticalTextAlignment="Center"/>


                            <Entry HeightRequest="38" Grid.Row="1" Grid.Column="1" IsEnabled="False" BackgroundColor="#f2f2f2" TextColor="Black"
                        MaxLength="32" VerticalTextAlignment="Center" ZIndex="5" Placeholder="{Binding LeagueNameStr}" PlaceholderColor="Black"/>

                            <Entry HeightRequest="38" Grid.Row="2" Grid.Column="1" IsEnabled="False" BackgroundColor="#f2f2f2" TextColor="Black"
                        MaxLength="32" VerticalTextAlignment="Center" ZIndex="5" Placeholder="{Binding CreatedOnDateStr}" PlaceholderColor="Black"/>



                            <Grid ColumnDefinitions="*,35" RowDefinitions="*" Grid.Row="3" Grid.Column="1">
                                <Entry HeightRequest="38" IsEnabled="False" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" VerticalOptions="Center" HorizontalOptions="FillAndExpand" BackgroundColor="#f2f2f2" TextColor="Black" ZIndex="5" Placeholder="My Team" PlaceholderColor="Black" VerticalTextAlignment="Center"/>
                                <CheckBox BackgroundColor="#6588a6" Color="#6588a6" IsEnabled="False" IsChecked="{Binding CreatedByCurrentUser}" VerticalOptions="Center" ZIndex="9" Grid.Column="1" HorizontalOptions="Start" WidthRequest="32" HeightRequest="32"/>
                            </Grid>


                        </Grid>
                    </DataTemplate>
                </CollectionView.ItemTemplate>
            </CollectionView>


        </Grid>

        <Grid Grid.Column="1" Grid.Row="3" Grid.RowSpan="1" ZIndex="1" x:Name="TeamsButtonsGrid" ColumnDefinitions=".5*,.5*" Margin="0,5,0,0">
            <HorizontalStackLayout Grid.Column="1" VerticalOptions="Center" Grid.ColumnSpan="1" HorizontalOptions="Start" Margin="7.5,0,0,0">
                <Button Text="VIEW TEAM" CornerRadius="0" BorderColor="#ffffff" BorderWidth="1" x:Name="ViewTeamBtn" Clicked="OnViewTeamBtnClicked" HeightRequest="44" WidthRequest="90" VerticalOptions="Center" HorizontalOptions="End" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6" TextColor="Black" Margin="0, 0, 0, 0"/>
                <Button Text="GO BACK" CornerRadius="0" BorderColor="#ffffff" BorderWidth="1" IsVisible="false" x:Name="GoBackTeamBtn" Clicked="OnGoBackTeamClicked" HeightRequest="44" WidthRequest="90" VerticalOptions="Center" HorizontalOptions="End" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6" TextColor="Black" Margin="0, 0, 0, 0"/>

                <Button Text="DELETE TEAM" CornerRadius="0" BorderColor="#ffffff" BorderWidth="1" x:Name="DeleteTeamBtn" Padding="-5" Clicked="OnDeleteTeamClicked" HeightRequest="44" WidthRequest="90" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6" TextColor="Black" Margin="2.5, 0, 0, 0"/>
                <Button Text="ENTER" CornerRadius="0" BorderColor="#ffffff" BorderWidth="1" x:Name="EnterTeamBtn" IsVisible="false" Padding="-5" Clicked="OnEnterTeamClicked" HeightRequest="44" WidthRequest="90" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6" TextColor="Black" Margin="2.5, 0, 0, 0"/>
                <Button Text="SIGN OUT" CornerRadius="0" BorderColor="#ffffff" BorderWidth="1" x:Name="SignoutBtn" Padding="-5" Clicked="OnSignoutClicked" HeightRequest="44" WidthRequest="90" VerticalOptions="Center" HorizontalOptions="Center" FontAutoScalingEnabled="True" FontSize="8" BackgroundColor="#e6e6e6" TextColor="Black" Margin="2.5, 0, 0, 0"/>

            </HorizontalStackLayout>
        </Grid>

        <Grid Grid.Column="1" Grid.Row="2" Grid.RowSpan="1" ZIndex="1" x:Name="TeamsInLeagueGrid" IsVisible="false">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width=".5*"/>
                <ColumnDefinition Width=".5*"/>
            </Grid.ColumnDefinitions>

            <CollectionView x:Name="TeamsInLeagueCollectionView" IsEnabled="False" Grid.Column="1" Grid.Row="0" WidthRequest="380" HorizontalOptions="StartAndExpand" SelectionMode="Single" SelectionChanged="TeamsInLeague_SelectionChanged" ZIndex="5" VerticalOptions="Center" EmptyView="No teams in league. Please create a team at this time.">
                <CollectionView.ItemsLayout>
                    <GridItemsLayout Orientation="Vertical" Span="1"/>
                </CollectionView.ItemsLayout>
                <CollectionView.ItemTemplate>
                    <DataTemplate>
                        <Grid Padding="10" VerticalOptions="Center">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="44"/>
                                <RowDefinition Height="44"/>
                                <RowDefinition Height="44"/>
                                <RowDefinition Height="44"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width=".1*"/>
                                <ColumnDefinition Width=".8*"/>
                                <ColumnDefinition Width=".1*"/>
                            </Grid.ColumnDefinitions>


                            <Entry HeightRequest="38" Grid.Row="0" Grid.Column="1" IsEnabled="False" VerticalOptions="Center" HorizontalOptions="FillAndExpand" BackgroundColor="#f2f2f2" TextColor="Black" ZIndex="5" Placeholder="{Binding TeamNameStr}" PlaceholderColor="Black" VerticalTextAlignment="Center"/>


                            <Entry HeightRequest="38" Grid.Row="1" Grid.Column="1" IsEnabled="False" BackgroundColor="#f2f2f2" TextColor="Black"
                        MaxLength="32" VerticalTextAlignment="Center" ZIndex="5" Placeholder="{Binding LeagueNameStr}" PlaceholderColor="Black"/>

                            <Entry HeightRequest="38" Grid.Row="2" Grid.Column="1" IsEnabled="False" BackgroundColor="#f2f2f2" TextColor="Black"
                        MaxLength="32" VerticalTextAlignment="Center" ZIndex="5" Placeholder="{Binding CreatedOnDateStr}" PlaceholderColor="Black"/>



                            <Grid ColumnDefinitions="*,35" RowDefinitions="*" Grid.Row="3" Grid.Column="1">
                                <Entry HeightRequest="38" IsEnabled="False" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" VerticalOptions="Center" HorizontalOptions="FillAndExpand" BackgroundColor="#f2f2f2" TextColor="Black" ZIndex="5" Placeholder="My Team" PlaceholderColor="Black" VerticalTextAlignment="Center"/>
                                <CheckBox BackgroundColor="#6588a6" Color="#6588a6" IsEnabled="False" IsChecked="{Binding CreatedByCurrentUser}" VerticalOptions="Center" ZIndex="9" Grid.Column="1" HorizontalOptions="Start" WidthRequest="32" HeightRequest="32"/>
                            </Grid>
                        </Grid>

                    </DataTemplate>
                </CollectionView.ItemTemplate>
            </CollectionView>


        </Grid>

        <Grid x:Name="CurrentPlayersCollectionViewGrid" IsVisible="false" Grid.Column="1" Grid.Row="2" Grid.RowSpan="1" ZIndex="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width=".5*"/>
                <ColumnDefinition Width=".5*"/>
            </Grid.ColumnDefinitions>

            <CollectionView x:Name="CurrentPlayersCollectionView" IsEnabled="false" SelectionMode="Single" SelectionChanged="CurrentPlayers_SelectionChanged" Grid.Column="0" Grid.Row="0" WidthRequest="380" HorizontalOptions="StartAndExpand" ZIndex="5" VerticalOptions="Center" EmptyView="No Players On Team. Add Free Agents.">
                <CollectionView.ItemsLayout>
                    <GridItemsLayout Orientation="Vertical" Span="1"/>
                </CollectionView.ItemsLayout>
                <CollectionView.ItemTemplate>
                    <DataTemplate>
                        <Grid Padding="10" VerticalOptions="Center">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="22"/>
                                <RowDefinition Height="44"/>
                                <RowDefinition Height="44"/>
                                <RowDefinition Height="44"/>
                                <RowDefinition Height="22"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width=".1*"/>
                                <ColumnDefinition Width=".8*"/>
                                <ColumnDefinition Width=".1*"/>
                            </Grid.ColumnDefinitions>


                            <BoxView Color="#ffffff" Grid.Row="0" Grid.RowSpan="1" Grid.Column="0" Grid.ColumnSpan="3">
                                <BoxView.Shadow>
                                    <Shadow Brush="black" Offset="0,10" Radius="20" Opacity="0.8"/>
                                </BoxView.Shadow>
                            </BoxView>

                            <BoxView Color="#ffffff" Grid.Row="4" Grid.RowSpan="1" Grid.Column="0" Grid.ColumnSpan="3">
                                <BoxView.Shadow>
